# While loop that creates the Audiocard variables
# Starts at Audiocard 1 and breaks if output is empty
n=
while true; do[8;39H[?12l[?25h[?25l[0m[19;55H~@k[8;39H[19;55H   [8;40H[19;68H40[8;40H[?12l[?25h[?25l[19;55H~@k[8;40H[19;55H   [8;41H[19;69H1[8;41H[?12l[?25h[?25l[19;55H~@k[8;41H[19;55H   [8;42H[19;69H2[8;42H[?12l[?25h[?25l[19;55Hx[8;42H[19;55H [8;42H[19;55Hdl[8;42H[19;55H  [8;42H[35m [0m[33m|[0m[35m awk [0m[33m'[0m[31m{ print $2 }[0m[33m'[0m[35m)[0m[8;64H[K[9;1H[33m[[0m [33m-n[0m [4C [33m][0m && [35m([0m[33mecho[0m[5C[31m----------[2CCard Number: [0m[2C[33m;[0m[35m pactl list short sources [0m[33m|[0m[35m [0m[33mg[10;1Hrep[0m[35m \^[1C [0m[33m|[0m[35m awk [0m[33m'[0m[8C[31m$2[0m[2C[33m'[0m[35m [0m[33m>[0m[35m [7C/ACsrc0[0m[33m;[0m[35m [0m[33mecho[0m[2C[31mSource: [0m[11;2H[33m;[0m[35m pactl list short sinks [0m[33m|[0m[35m [0m[33mgrep[0m[35m \^[1C [0m[33m|[0m[35m awk [0m[33m'[0m[8C[31m$2[0m[2C[33m'[0m[35m [0m[33m>[0m[35m [7C/ACsnk0[0m[35m)[0m; [33mecho[0m[12;2H[31mSink: [0m[22C;


